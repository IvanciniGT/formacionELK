{
    "index_patterns": ["apache-logs-nuevos-*"],
    "priority": 200,
    "template": {
      "settings": {
        "index":{
          "number_of_shards": 3,
          "number_of_replicas": 1
        }
      },
      "mappings": {
          "properties": {
            "source": {
              "properties": {
                "address": {
                  "type": "ip"
                }
              }
            },
            "geoip": {
              "properties": {
                "geo": {
                  "properties": {
                    "location": {
                      "type": "geo_point"
                    }
                  }
                }
              }
            }
          }
      }
  }
}